{
  "name": "noobaa-core",
  "version": "2.11.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "main": "index.js",
  "config": {
    "covdir": "coverage"
  },
  "scripts": {
    "preinstall": "npm install node-addon-api nan",
    "postinstall": "npm run build",
    "---": "------------------------------------------------------------------",
    "build:package": "gulp package_build",
    "build:native": "node-gyp rebuild",
    "build:fe": "node src/tools/npm_install.js frontend",
    "build": "npm run build:native && npm run build:fe",
    "----": "-----------------------------------------------------------------",
    "lint": "eslint src --quiet",
    "mocha": "node --allow-natives-syntax ./node_modules/.bin/_mocha src/test/unit_tests/index.js",
    "mocha:coverage": "nyc --all --include 'src/**/*.js' --exclude 'src/util/mongo_functions.js' --report-dir $npm_package_config_covdir --reporter lcov npm run mocha",
    "test:web": "mocha src/test/web/index.js",
    "test": "npm run lint && npm run mocha:coverage",
    "-----": "----------------------------------------------------------------",
    "db": "mongod -f mongod.conf",
    "web": "node src/server/web_server",
    "bg": "node src/server/bg_workers",
    "s3": "node src/s3/s3rver_starter",
    "hosted_agents": "node src/hosted_agents/hosted_agents_starter",
    "tunnel": "sudo node src/tools/tcp_tunnel",
    "agent": "node src/agent/agent_cli",
    "agents": "node src/agent/agent_cli --scale 10",
    "start": "node src/server",
    "------": "---------------------------------------------------------------",
    "clean:storage": "rm -rf ./metadata_storage/* ./noobaa_storage* ./agent_conf*.json",
    "clean:garbage": "find . -name .DS_Store -print -delete",
    "clean:install": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./build",
    "clean:test": "rm -rf ./coverage ./logs",
    "clean:fe": "rm -rf ./frontend/node_modules ./frontend/dist ./frontend/src/lib",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:test && npm run clean:fe && npm run clean:garbage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noobaa/noobaa-core.git"
  },
  "bugs": {
    "url": "https://github.com/noobaa/noobaa-core/issues"
  },
  "homepage": "https://github.com/noobaa/noobaa-core",
  "browser": {
    "express": false,
    "morgan": false,
    "compression": false,
    "bindings": false,
    "ws": false,
    "./src/util/nb_native": false,
    "./config-local": false
  },
  "dependencies": {
    "@google-cloud/storage": "2.3.4",
    "ajv": "6.6.2",
    "aws-sdk": "2.382.0",
    "azure-storage": "2.10.2",
    "bcrypt": "3.0.3",
    "big-integer": "1.6.40",
    "bindings": "1.3.1",
    "bluebird": "3.5.3",
    "bufferutil": "4.0.1",
    "chance": "1.0.18",
    "compression": "1.7.3",
    "dclassify": "1.1.2",
    "eslint": "5.11.0",
    "eslint-plugin-header": "2.0.0",
    "event-stream": "4.0.1",
    "express": "4.16.4",
    "forever": "0.15.3",
    "forever-service": "0.5.11",
    "ftp-srv": "3.0.5",
    "get-folder-size": "2.0.0",
    "getos": "3.1.1",
    "glob-to-regexp": "0.4.0",
    "googleapis": "36.0.0",
    "gulp": "4.0.0",
    "gulp-gzip": "1.4.2",
    "gulp-json-editor": "2.4.4",
    "gulp-rename": "1.4.0",
    "gulp-tar": "2.1.0",
    "gulp-util": "3.0.8",
    "heapdump": "0.3.12",
    "http-proxy-agent": "2.1.0",
    "https-proxy-agent": "2.2.1",
    "ini": "1.3.5",
    "ip": "1.1.5",
    "jsonwebtoken": "8.4.0",
    "linux-blockutils": "0.1.0",
    "lodash": "4.17.11",
    "mime": "2.4.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "moment": "2.23.0",
    "moment-timezone": "0.5.23",
    "mongodb": "3.1.10",
    "mongodb-uri": "0.9.7",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "nan": "2.12.1",
    "ncp": "2.0.0",
    "net-ping": "1.2.3",
    "node-addon-api": "1.6.2",
    "node-df": "0.1.4",
    "node-gyp": "3.8.0",
    "performance-now": "2.1.0",
    "ps-node": "0.1.6",
    "request": "2.88.0",
    "rimraf": "2.6.2",
    "serve-favicon": "2.5.0",
    "setimmediate": "1.0.5",
    "sinon": "7.2.2",
    "ssh2": "0.6.1",
    "utf-8-validate": "5.0.2",
    "uuid": "3.3.2",
    "vsphere": "https://download3.vmware.com/software/vmw-tools/vsphere-sdk-for-javascript/vsphere-1.1.0.tgz",
    "why-is-node-running": "2.0.3",
    "winston": "3.1.0",
    "ws": "6.1.2",
    "xml2js": "0.4.19",
    "yauzl": "2.10.0",
    "yazl": "2.5.1"
  },
  "devDependencies": {
    "azure": "2.3.1-preview",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "chromedriver": "2.45.0",
    "istanbul-lib-coverage": "2.0.1",
    "istanbul-lib-hook": "2.0.1",
    "istanbul-lib-instrument": "3.0.0",
    "istanbul-lib-report": "2.0.2",
    "istanbul-reports": "2.0.1",
    "nyc": "13.1.0",
    "phantomjs-prebuilt": "2.1.16",
    "seedrandom": "2.4.4",
    "selenium-standalone": "6.15.4",
    "selenium-webdriver": "3.6.0"
  }
}
