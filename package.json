{
  "name": "noobaa-core",
  "version": "2.8.2",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "engines": {
    "node": "8.9.3"
  },
  "main": "index.js",
  "config": {
    "covdir": "coverage"
  },
  "scripts": {
    "preinstall": "npm install node-addon-api nan",
    "postinstall": "npm run build",
    "---": "------------------------------------------------------------------",
    "build:package": "gulp package_build",
    "build:native": "node-gyp rebuild",
    "build:fe": "node src/tools/npm_install.js frontend",
    "build": "npm run build:native && npm run build:fe",
    "----": "-----------------------------------------------------------------",
    "lint": "eslint src --quiet",
    "mocha": "node --allow-natives-syntax ./node_modules/.bin/_mocha src/test/unit_tests/index.js",
    "mocha:coverage": "nyc --all --include 'src/**/*.js' --exclude 'src/util/mongo_functions.js' --report-dir $npm_package_config_covdir --reporter lcov npm run mocha",
    "test:web": "mocha src/test/web/index.js",
    "test": "npm run lint && npm run mocha:coverage",
    "-----": "----------------------------------------------------------------",
    "db": "mongod -f mongod.conf",
    "web": "node src/server/web_server",
    "bg": "node src/server/bg_workers",
    "s3": "node src/s3/s3rver_starter",
    "hosted_agents": "node src/hosted_agents/hosted_agents_starter",
    "tunnel": "sudo node src/tools/tcp_tunnel",
    "agent": "node src/agent/agent_cli",
    "agents": "node src/agent/agent_cli --scale 10",
    "start": "node src/server",
    "------": "---------------------------------------------------------------",
    "clean:db": "rm -rf ./noobaa_storage/* ./metadata_storage/* ./agent_conf.json",
    "clean:garbage": "find . -name .DS_Store -print -delete",
    "clean:install": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./build",
    "clean:test": "rm -rf ./coverage ./logs",
    "clean:fe": "rm -rf ./frontend/node_modules ./frontend/dist ./frontend/src/lib",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:test && npm run clean:fe && npm run clean:garbage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noobaa/noobaa-core.git"
  },
  "bugs": {
    "url": "https://github.com/noobaa/noobaa-core/issues"
  },
  "homepage": "https://github.com/noobaa/noobaa-core",
  "browser": {
    "express": false,
    "morgan": false,
    "compression": false,
    "bindings": false,
    "ws": false,
    "./src/util/nb_native": false,
    "./config-local": false
  },
  "dependencies": {
    "@google-cloud/storage": "1.6.0",
    "ajv": "5.5.2",
    "aws-sdk": "2.175.0",
    "azure-storage": "2.7.0",
    "bcrypt": "1.0.3",
    "big-integer": "1.6.26",
    "bindings": "1.3.0",
    "bluebird": "3.5.1",
    "bufferutil": "3.0.3",
    "chance": "1.0.12",
    "compression": "1.7.1",
    "dclassify": "1.1.2",
    "eslint": "4.14.0",
    "eslint-plugin-header": "1.2.0",
    "event-stream": "3.3.4",
    "express": "4.16.2",
    "forever": "0.15.3",
    "forever-service": "0.5.11",
    "ftp-srv": "2.14.0",
    "get-folder-size": "1.0.1",
    "getos": "3.1.0",
    "glob-to-regexp": "0.3.0",
    "googleapis": "23.0.0",
    "gulp": "3.9.1",
    "gulp-gzip": "1.4.0",
    "gulp-json-editor": "2.2.1",
    "gulp-rename": "1.2.2",
    "gulp-tar": "2.0.0",
    "gulp-util": "3.0.8",
    "heapdump": "0.3.9",
    "http-proxy-agent": "2.0.0",
    "https-proxy-agent": "2.1.1",
    "ini": "1.3.5",
    "ip": "1.1.5",
    "jsonwebtoken": "8.1.0",
    "linux-blockutils": "0.1.0",
    "lodash": "4.17.4",
    "mime": "2.1.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "4.0.1",
    "moment": "2.20.1",
    "moment-timezone": "0.5.14",
    "mongodb": "2.2.33",
    "mongodb-uri": "0.9.7",
    "morgan": "1.9.0",
    "multer": "1.3.0",
    "nan": "2.8.0",
    "ncp": "2.0.0",
    "net-ping": "1.2.1",
    "node-addon-api": "1.1.0",
    "node-df": "0.1.1",
    "node-gyp": "3.6.2",
    "performance-now": "2.1.0",
    "ps-node": "0.1.6",
    "request": "2.83.0",
    "rimraf": "2.6.2",
    "serve-favicon": "2.4.5",
    "setimmediate": "1.0.5",
    "sinon": "4.3.0",
    "ssh2": "0.5.5",
    "utf-8-validate": "4.0.0",
    "uuid": "3.1.0",
    "vsphere": "https://download3.vmware.com/software/vmw-tools/vsphere-sdk-for-javascript/vsphere-1.1.0.tgz",
    "why-is-node-running": "1.2.3",
    "winston": "2.4.2",
    "ws": "3.3.3",
    "xml2js": "0.4.19",
    "yauzl": "2.9.1",
    "yazl": "2.4.3"
  },
  "devDependencies": {
    "azure": "2.2.1-preview",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "chromedriver": "2.34.0",
    "istanbul-lib-coverage": "1.1.1",
    "istanbul-lib-hook": "1.1.0",
    "istanbul-lib-instrument": "1.9.1",
    "istanbul-lib-report": "1.1.2",
    "istanbul-reports": "1.1.3",
    "nyc": "11.4.1",
    "phantomjs-prebuilt": "2.1.16",
    "selenium-standalone": "6.12.0",
    "selenium-webdriver": "3.6.0"
  }
}
