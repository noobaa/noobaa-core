    "mongodb": "3.2.7",
    "mongodb-uri": "0.9.7",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "nan": "2.14.1",
    "ncp": "2.0.0",
    "net-ping": "1.2.3",
