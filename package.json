{
  "name": "noobaa-core",
  "version": "5.13.0",
  "engines": { "node": "18.12.1" },
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "main": "src/bin/nsfs.js",
  "bin": "src/bin/nsfs.js",
  "config": {
    "covdir": "coverage"
  },
  "scripts": {
    "install": "echo install hook override to avoid npm default hook calling node-gyp",
    "---": "------------------------------------------------------------------",
    "build:native": "node-gyp rebuild",
    "build:bin": "pkg . --public",
    "build": "npm run build:native",
    "----": "-----------------------------------------------------------------",
    "lint": "eslint src --quiet",
    "ts": "tsc && tslint -p tsconfig.json -c tslint.json",
    "mocha": "node --allow-natives-syntax ./node_modules/.bin/_mocha src/test/unit_tests/index.js",
    "mocha:coverage": "nyc --all --include 'src/**/*.js' --exclude 'src/util/mongo_functions.js' --report-dir $npm_package_config_covdir --reporter lcov npm run mocha",
    "test": "npm run lint && npm run mocha",
    "-----": "----------------------------------------------------------------",
    "start": "npm run nsfs",
    "nsfs": "node src/bin/nsfs",
    "nscache": "node src/bin/nscache",
    "api": "node src/bin/api",
    "------": "---------------------------------------------------------------",
    "web": "node src/server/web_server",
    "bg": "node src/server/bg_workers",
    "s3": "node src/s3/s3rver_starter",
    "s3:port80tunnel": "sudo node src/tools/tcp_tunnel",
    "hosted_agents": "node src/hosted_agents/hosted_agents_starter",
    "agent": "node src/agent/agent_cli",
    "agents": "node src/agent/agent_cli --scale 10",
    "backingstore": "node src/agent/backingstore",
    "-------": "--------------------------------------------------------------",
    "db:clean": "rm -rf db_storage/",
    "db:init": "initdb -D db_storage/ -E UTF-8 --locale=C",
    "db": "postgres -D db_storage/",
    "db:createuser": "createuser postgres && psql postgres -c 'ALTER USER postgres CREATEDB'",
    "db:createdb": "createdb -U postgres nbcore",
    "db:grantperm": "psql nbcore postgres -c 'GRANT ALL ON SCHEMA public TO postgres'",
    "db:create": "npm run db:createuser && npm run db:createdb",
    "db:connect": "psql nbcore postgres",
    "--------": "-------------------------------------------------------------",
    "clean:storage": "rm -rf ./db_storage/ ./noobaa_storage* ./agent_conf*.json",
    "clean:garbage": "find . -name .DS_Store -print -delete",
    "clean:install": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./build",
    "clean:test": "rm -rf ./coverage ./logs",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:test && npm run clean:garbage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noobaa/noobaa-core.git"
  },
  "bugs": {
    "url": "https://github.com/noobaa/noobaa-core/issues"
  },
  "homepage": "https://github.com/noobaa/noobaa-core",
  "browser": {
    "express": false,
    "morgan": false,
    "compression": false,
    "bindings": false,
    "ws": false,
    "./src/util/nb_native": false,
    "./config-local": false
  },
  "pkg": {
    "outputPath": "build",
    "targets": [
      "macos",
      "linux"
    ],
    "assets": [
      "node_modules/**/*.node",
      "build/**/*.node"
    ]
  },
  "dependencies": {
    "@azure/storage-blob": "12.12.0",
    "@google-cloud/storage": "6.8.0",
    "ajv": "8.11.2",
    "aws-sdk": "2.1286.0",
    "azure-storage": "2.10.7",
    "bcrypt": "5.1.0",
    "big-integer": "1.6.51",
    "bindings": "1.5.0",
    "bufferutil": "4.0.7",
    "chance": "1.1.9",
    "compression": "1.7.4",
    "express": "4.18.2",
    "express-http-proxy": "1.6.3",
    "getos": "3.2.1",
    "glob-to-regexp": "0.4.1",
    "heapdump": "0.3.15",
    "http-proxy-agent": "5.0.0",
    "https-proxy-agent": "5.0.1",
    "ip": "1.1.8",
    "jsonwebtoken": "9.0.0",
    "linux-blockutils": "0.2.0",
    "lodash": "4.17.21",
    "mime": "3.0.0",
    "minimist": "1.2.7",
    "moment": "2.29.4",
    "moment-timezone": "0.5.40",
    "mongo-query-to-postgres-jsonb": "0.2.15",
    "mongodb": "3.7.3",
    "morgan": "1.10.0",
    "nan": "2.17.0",
    "ncp": "2.0.0",
    "net-ping": "1.2.3",
    "node-addon-api": "5.0.0",
    "node-df": "0.1.4",
    "node-gyp": "9.3.1",
    "performance-now": "2.1.0",
    "pg": "8.8.0",
    "prom-client": "14.1.1",
    "ps-node": "0.1.6",
    "request": "2.88.2",
    "rimraf": "3.0.2",
    "seedrandom": "3.0.5",
    "setimmediate": "1.0.5",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typescript": "4.9.4",
    "utf-8-validate": "5.0.10",
    "uuid": "9.0.0",
    "ws": "8.11.0",
    "xml2js": "0.4.23",
    "yaml": "2.2.1",
    "yamljs": "0.3.0",
    "yauzl": "2.10.0",
    "yazl": "2.5.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.191",
    "@types/mongodb": "3.6.20",
    "@types/node": "18.11.18",
    "@types/pg": "8.6.6",
    "@types/request": "2.48.8",
    "eslint": "8.23.1",
    "eslint-plugin-header": "3.1.1",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-hook": "3.0.0",
    "istanbul-lib-instrument": "5.2.1",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.5",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "pkg": "5.8.0",
    "prebuild-install": "7.1.1",
    "sinon": "15.0.1",
    "wtfnode": "0.9.1"
  }
}
