{
  "name": "noobaa-core",
  "version": "2.0.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "engines": {
    "node": "6.11.5"
  },
  "main": "index.js",
  "config": {
    "covdir" : "coverage"
  },
  "scripts": {
    "preinstall": "npm install node-addon-api nan",
    "postinstall": "npm run build",
    "---": "------------------------------------------------------------------",
    "build:package": "gulp package_build",
    "build:native": "node-gyp rebuild",
    "build:fe": "node src/tools/npm_install.js frontend",
    "build": "npm run build:native && npm run build:fe",
    "----": "-----------------------------------------------------------------",
    "lint": "eslint src --quiet",
    "mocha": "node --allow-natives-syntax ./node_modules/.bin/_mocha src/test/unit_tests/index.js",
    "mocha:coverage": "nyc --all --include 'src/**/*.js' --exclude 'src/util/mongo_functions.js' --report-dir $npm_package_config_covdir --reporter lcov npm run mocha",
    "test:web": "mocha src/test/web/index.js",
    "test": "npm run lint && npm run mocha:coverage",
    "-----": "----------------------------------------------------------------",
    "db": "mongod -f mongod.conf",
    "web": "node src/server/web_server",
    "bg": "node src/server/bg_workers",
    "s3": "node src/s3/s3rver_starter",
    "hosted_agents": "node src/hosted_agents/hosted_agents_starter",
    "tunnel": "sudo node src/tools/tcp_tunnel",
    "agent": "node src/agent/agent_cli",
    "agents": "node src/agent/agent_cli --scale 10",
    "start": "node src/server",
    "------": "---------------------------------------------------------------",
    "clean:storage": "rm -rf ./noobaa_storage*/ ./metadata_storage/* agent_conf*.json",
    "clean:garbage": "find . -name .DS_Store -print -delete",
    "clean:install": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./build",
    "clean:test": "rm -rf ./coverage ./logs",
    "clean:fe": "rm -rf ./frontend/node_modules ./frontend/dist ./frontend/src/lib",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:test && npm run clean:fe && npm run clean:garbage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noobaa/noobaa-core.git"
  },
  "bugs": {
    "url": "https://github.com/noobaa/noobaa-core/issues"
  },
  "homepage": "https://github.com/noobaa/noobaa-core",
  "browser": {
    "nb_native": false,
    "nb_native_nan": false,
    "express": false,
    "morgan": false,
    "compression": false,
    "bindings": false,
    "ws": false,
    "./config-local": false
  },
  "dependencies": {
    "ajv": "5.2.0",
    "aws-sdk": "2.80.0",
    "azure-storage": "2.2.1",
    "bcrypt": "1.0.2",
    "big-integer": "1.6.23",
    "bindings": "1.3.0",
    "bluebird": "3.5.0",
    "bufferutil": "3.0.1",
    "chance": "1.0.10",
    "compression": "1.6.2",
    "dclassify": "1.1.2",
    "eslint": "4.13.1",
    "eslint-plugin-header": "1.2.0",
    "event-stream": "3.3.4",
    "express": "4.15.3",
    "forever": "0.15.3",
    "forever-service": "0.5.9",
    "getos": "3.0.1",
    "glob-to-regexp": "0.1.0",
    "googleapis": "19.0.0",
    "gulp": "3.9.1",
    "gulp-gzip": "1.4.0",
    "gulp-json-editor": "2.2.1",
    "gulp-rename": "1.2.2",
    "gulp-tar": "1.9.0",
    "gulp-util": "3.0.7",
    "heapdump": "0.3.9",
    "ip": "1.1.5",
    "jsonwebtoken": "7.4.1",
    "linux-blockutils": "0.1.0",
    "lodash": "4.17.4",
    "mime": "1.3.6",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "4.0.1",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "mongodb": "2.2.33",
    "mongodb-uri": "0.9.7",
    "morgan": "1.7.0",
    "multer": "1.1.0",
    "nan": "2.7.0",
    "ncp": "2.0.0",
    "net-ping": "1.2.0",
    "node-addon-api": "0.6.3",
    "node-df": "0.1.1",
    "node-gyp": "3.6.2",
    "performance-now": "2.1.0",
    "proxy-agent": "2.1.0",
    "request": "2.81.0",
    "rimraf": "2.6.1",
    "serve-favicon": "2.4.3",
    "setimmediate": "1.0.5",
    "ssh2": "0.5.5",
    "utf-8-validate": "3.0.2",
    "uuid": "3.1.0",
    "vsphere": "https://download3.vmware.com/software/vmw-tools/vsphere-sdk-for-javascript/vsphere-1.1.0.tgz",
    "why-is-node-running": "1.2.3",
    "winston": "1.1.2",
    "ws": "3.0.0",
    "xml2js": "0.4.17",
    "yauzl": "2.8.0",
    "yazl": "2.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "chromedriver": "2.33.2",
    "istanbul-lib-coverage": "1.1.1",
    "istanbul-lib-hook": "1.1.0",
    "istanbul-lib-instrument": "1.9.1",
    "istanbul-lib-report": "1.1.2",
    "istanbul-reports": "1.1.3",
    "nyc": "11.3.0",
    "phantomjs-prebuilt": "2.1.16",
    "selenium-standalone": "6.12.0",
    "selenium-webdriver": "3.6.0"
  }
}
