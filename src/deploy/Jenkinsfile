node('node2') {
   stage('GetLatestDataSet') { // for display purposes
      echo 'Grabbing latest build from jenkins!'
      sh 'curl -u tamireran:0436dd1acfaf9cd247b3dd22a37f561f -L http://146.148.16.59:8080/job/mdserver/lastSuccessfulBuild/artifact/*zip*/archive.zip >/tmp/noobaa.zip'
      sh 'tar -xzvf /tmp/noobaa.zip'

   }
   stage('RunDataSet') {
      // Run the maven build
      echo 'Running some dataSet!'
      sh 'node /tmp/noobaa-core/src/test/qa/dataset.js --server 13.92.133.78'
   }
}
