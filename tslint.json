{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": []
    },

    "rules": {
        "align": false,
        "curly": false,
        "class-name": false,
        "interface-name": false,
        "max-line-length": false,
        "no-console": false,
        "no-reference": false,
        "no-namespace": false,
        "no-empty-interface": false,
        "no-consecutive-blank-lines": false,
        "no-trailing-whitespace": [
            true,
            "ignore-comments",
            "ignore-jsdoc",
            "ignore-template-strings",
            "ignore-blank-lines"
        ],
        "object-literal-sort-keys": false,
        "quotemark": false,
        "trailing-comma": false,
        "variable-name": false
    },

    "jsRules": {
        "align": false,
        "curly": false,
        "class-name": false,
        "interface-name": false,
        "max-line-length": false,
        "no-console": false,
        "no-reference": false,
        "no-namespace": false,
        "no-empty-interface": false,
        "no-consecutive-blank-lines": false,
        "no-trailing-whitespace": [
            true,
            "ignore-comments",
            "ignore-jsdoc",
            "ignore-template-strings",
            "ignore-blank-lines"
        ],
        "object-literal-sort-keys": false,
        "quotemark": false,
        "trailing-comma": false,
        "variable-name": false
    }
}
